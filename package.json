{
  "name": "hugo-theme-dream",
  "version": "3.0.0",
  "description": "A Hugo theme named Dream.",
  "repository": "https://github.com/g1eny0ung/hugo-theme-dream.git",
  "author": "Yue Yang <g1enyy0ung@gmail.com> (https://g1en.site)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "css": "tailwindcss -i ./src/input.css -o ./static/css/output.css",
    "js": "babel src --out-dir static --presets=@babel/preset-env",
    "watch": "concurrently \"npm:css -- -w\" \"npm:js -- -w\""
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "concurrently": "^8.0.1",
    "daisyui": "^4.11.1",
    "tailwindcss": "^3.3.1"
  }
}
